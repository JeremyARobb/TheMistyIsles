	public void exploreF1() {
		if (Chicken2 == false) {
			System.out.println("1) BanditCoop");
		}
		if (Mugging == true) {
			System.out.println("2) Backalley street");
		}
		if (Mugging == false) {
			System.out.println("2) ???");
		}
		if (bureaucracy == false) {
			System.out.println("3) ???");
		}
		if (bureaucracy == true) {
			System.out.println("3) Waterhole");
		}
		if (MasterOogway == false) {
			System.out.println("4) ???");
		}
		if (MasterOogway == true) {
			System.out.println("4) Pagoda");
		}
		answer = choice.next();
		if (answer.equals("5") && MagicP1 == true && MagicP2 == true && MagicP3 == true) {
			
		}
		if (answer.equals("4")) {
			if (MasterOogway == false) {
				System.out.println("You come upon a pagoda, though old you can see it has not been forgotten, a great garden full of every color and every plantable food flows around the pagoda reminding you of a serene river on a quiet, sunny, spring morning. As you approach the gate you see that it's locked, and the fence is built in the way that you cant climb over, to get through you're going to need the key.");
				MasterOogway = true;
			}
			System.out.println("What would you like to do with the gate?");
			System.out.println("1) Leave");
			System.out.println("2) Try to force the lock");
