			MagicP3 = true;
			}
			answer = "90";
		}//end pagoda
		if (answer.equals("3") && Blost == true) {
			System.out.println("You decide not to go back to the watering hole after remembering that you had your pass stolen by a robovorski hamster");
		}
		if (answer.equals("3") && saturated == true) {
			System.out.println("You drink some more");
		}
		if (answer.equals("3") && Bwon == true && saturated == false) {
			System.out.println("You go and drink to your hearts content");
			System.out.println("Gained perk: Saturated, you still have a bit of room incase you happen upon some shire | +1 mana");
			saturated = true;
		}
		if (answer.equals("3") && BDone == false) {
			System.out.println("You discovered the waterhole, finally, you were beginning to think you might die of dehydration. As you approach you ask the gatekeeper for entry, they say you can come in, you just need to fill out a visitor form.");
			System.out.println("");
			bureaucracy = true;
			while (BDone == false) {
				System.out.println("'First name?'");
				System.out.println("1) " + Fname);
				System.out.println("2) Larry");
				System.out.println("3) " + Lname);
				System.out.println("4) Hagatha");
				System.out.println("5) Rick");
				answer = choice.next();
				if (answer.equals("4")) {
					B1 = true;
