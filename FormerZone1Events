public class FormerZone1Events extends SubsequentZone1Events{


	public void pub() {
		System.out.println("You enter the pub. ");
		while (Pub == false) {
			System.out.println("Which way do you go in the pub?");
			System.out.println("1) turn left");
			System.out.println("2) turn right");
			System.out.println("3) exit");
			if (cocowork == true && cocobowl == false) {
				System.out.println("4) hand the inkeep the bowl");
			}
			answer = choice.next();
			if (answer.equals("4") && cocowork == true && cocobowl == false) {
				System.out.println("The inkeep remarks that the Coockoo bird is to the left, and hands you back the bowl, which is now full of cocoa puffs");
				cocobowl = true;
				answer = "90";
			}
			if (answer.equals("3")) {
				Pub = true;
				answer = "90";
			}
			if (answer.equals("1") && drunk == true && coco == true) {
				System.out.println("You continually destroy your liver");
			}
			if (answer.equals("1") && coco == true && drunk == false) {
				System.out.println("You sit down for a drink");
				System.out.println("Earned a perk: 'Im not drunk, you're Jon Snow' | +1 hp");
				drunk = true;
				hp += 1;
			}
			if (answer.equals("1") && coco == false) {
				System.out.println("A crazy bird yelling that he's going 'Coockoo for Cocoa Puffs!' sees you and runs at you!");
				System.out.println("1) Run away!");
				System.out.println("2) Chaaaaaaaaaaarge! (attack)");
				if (cocobowl == true) {
					System.out.println("3) Dramatically whip out your bowl of Cocoa Puffs.");
				}
				answer = choice.next();
				if (answer.equals("3") && cocobowl == true) {
					System.out.println("The Coockoo bird jumps at you in a blinding flash, you black out... when you wake up you see the Coockoo bird eating his bowl of cocoa puffs calmly. 'What next,' you think 'A deranged rabbit who's craving another type of sugary goodness?'");
					cocoalive = true;
					coco = true;

				}
				if (answer.equals("1")) {
					System.out.println("You quickly exit the bar");
					Pub = true;
					answer = "90";
				}
				if (answer.equals("2")){
					fight(1, "CoockooBird", "none", "none", level, 15);
				
				if (win == false) {
					System.out.println("You wake up a couple of hours later dazed and confused");
				}
				if (win == true) {
					System.out.println("You knock out the crazy Coockoo bird, 'What next,' you think 'A deranged rabbit who's craving another type of sugary goodness?'");
					cocodead = true;
					coco = true;
					win = false;
				}
				}
			}
			if (answer.equals("2")) {
				if (regurgitatery1 == true) {
					System.out.println("Oh no you don't");
				}
				if (regurgitatery1 == false) {
					System.out.println("Nothing to see here, just a regurgitatery *Shudders*");
					System.out.println("1) Whats that?");
					System.out.println("2) Ew, I'm not going anywhere near that");
					answer = choice.next();
					if (answer.equals("1")) {
						System.out.println("Its horrible, its where the animals such as owls and seabirds regurgitate the bones of the meal they just ate");
						System.out.println("1) Inspect the regurgitatery");
						System.out.println("2) back away slowly");
						answer = choice.next();
						if (answer.equals("1")) {
							System.out.println("What are you doing, theres flies by there, not the average house fly either, these things have lived here their whole life, heck they were probably born right here in this hot, moist sack."
									+ " If one even touches you it could give you all numbers of diseases, and dont even get me started on the smell, oh god, the smell. You can litteraly see small green lines wiggling upwards as if even they want to escape the smell, and they litteraly are the smell. What kind of sane animal would get anywhere near that thing");
							System.out.println("1) Owls and seabirds (inspect closer)");
							System.out.println("2) you know what, you're right (leave)");
							answer = choice.next();
							if (answer.equals("1")) {
								System.out.print("No, what are you doing! You think just because you're the protagonist of a fictional story you cant die a horrible death in the tutorial? think of your life, your friends, your kids if you have any for peetes sake!");
								System.out.print(" I just wanted a normal player to play this, but no, I got a player named " + Fname + " ");
								nickname();
								System.out.println(Lname + ", Agh. Hey whats that at the bottom of the sack, the greenish redish elasticy thing");
								System.out.println("1) Grab it!");
								System.out.println("2) Listen to the narrator, the controller of your destiny for crying out loud");
								answer = choice.next();
								if (answer.equals("1")) {
									System.out.println("You recieved an item: Gut laced headband");
									System.out.println("");
									regurgitatery1 = true;
									GutHeadband = true;
								}
							}
							if (answer.equals("2")) {
								System.out.println("Took you long enough");
								System.out.println("");
							}
						}
						if (answer.equals("2")) {
							System.out.println("");
						}
					}
					if (answer.equals("2")) {
						System.out.println("Good choice");
						System.out.println("");
					}
				}
			}
		}
		Pub = false;
	}
